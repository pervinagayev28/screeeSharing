<Window x:Class="ClientSideWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientSideWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="0,20">
        <TextBox Height="40" Text="Who?" FontSize="20" FontWeight="Bold" HorizontalContentAlignment="Center" Name="txtbox" Width="400" />
        <Button IsEnabled="False" Height="100" Width="400" Content="Start Shareing Scree" FontSize="30" Click="Clicked" x:Name="btn"/>
        <!--<MediaElement Height="400" Width="500" Name="img" Visibility="Visible"/>-->
        <Image Name="img" Visibility="Visible"/>
        

    </StackPanel>
</Window>
